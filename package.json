{
  "name": "generic-data-table",
  "version": "1.0.0",
  "main": "dist/GenericDataTable.js",
  "typings": "dist/index.d.ts",
  "author": "Simply Smart Engineering ",
  "license": "MIT",
  "sideEffects": false,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "4.9.1",
    "@material-ui/styles": "4.10.0",
    "@material-ui/system": "4.9.14",
    "styled-components": "^5.2.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.8",
    "@types/react": "^16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
    "@typescript-eslint/parser": "^4.1.1",
    "esbuild": "^0.7.13",
    "husky": "^4.3.0",
    "lint-staged": "^10.5.1",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-clean-easy": "^1.0.2",
    "prettier": "2.2.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "typescript": "^4.0.3"
  },
  "parcelCleanPaths": [
    "dist/*.*"
  ],
  "scripts": {
    "start": "parcel ./example/app/index.html --open --port 3005",
    "fastbuild": "esbuild ./src/GenericDataTable.tsx --bundle --outdir=dist --define:process.env.NODE_ENV=\"production\" && cp ./src/types.d.ts ./dist/ ",
    "fastbuild-windows": "esbuild app.jsx --bundle --outfile=out.js",
    "format": "prettier --write **/*.ts{,x}",
    "typecheck": "tsc --noEmit",
    "test": "jest --coverage",
    "lint-staged": "lint-staged",
    "build": "tsc",
    "check": "npm run lint && npm test",
    "lint": "tsc --noEmit && esbuild ./src/**/*.{ts,tsx}",
    "lint:fix": "eslint ./src/**/*.{ts,tsx} --quiet --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged",
      "post-commit": "npm outdated"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
